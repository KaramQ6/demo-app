# متغيرات البيئة للتطبيق
# Environment Variables

# Google Places API Key للحصول على بيانات الازدحام الحقيقية
# احصل على المفتاح من: https://console.cloud.google.com/
# فعّل Places API في مشروعك
REACT_APP_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# n8n Weather API
REACT_APP_WEATHER_API_URL=https://n8n.smart-tour.app/webhook/Simple-Weather-API-Live-Data

# إعدادات أخرى
REACT_APP_APP_NAME=Smart Tourism Jordan
REACT_APP_VERSION=1.0.0

# تطوير فقط - لا تستخدم في الإنتاج
GENERATE_SOURCEMAP=false
